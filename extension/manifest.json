{
    "manifest_version": 3,
    "name": "test",
    "version": "test",
    "description": "test",
    "host_permissions": [ "https://www.youtube.com/*"],
    "content_scripts":
    [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["main.js"],
        "run_at": "document_idle"
      }
    ],
    "web_accessible_resources":
    [
      {
        "resources":
        [
          "assets/images/*.png"
        ],
        "matches": ["*://*/*"]
      }]
}